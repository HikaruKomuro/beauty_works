<div class="form-new">
	<h2 class="center form-title">オーナー　プロフィール編集</h2>
	<%= form_with(model: @owner, url: owners_registration_path(@owner), method: :patch, local: true) do |f| %>
		<%= render "errors_form", object: @owner %>
	  <%= render "form", f: f %>
	  <%= f.submit "更新する", class: "btn form-submit" %>
	<% end %>
</div>