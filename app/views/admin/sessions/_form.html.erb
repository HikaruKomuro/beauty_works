<!--input表示-->
<%= f.label :email, "メールアドレス", class: "form-label" %>
<%= f.email_field :email, class: "form-input form-control", required: true %>
<%= f.label :password, "パスワード", class: "form-label" %>
<%= f.password_field :password, class: "form-input form-control", required: true %>
<%= f.submit "ログイン", class: "btn btn-primary form-input" %>